{
    "name": "lirik44/phpunit-retry-annotations",
    "description": "Traits for retrying test methods and classes in PHPUnit, modified for selenium php-webdriver",
    "keywords": ["test", "retry", "phpunit", "php-webdriver"],
    "homepage": "https://github.com/lirik44/phpunit-retry-annotations",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kirill Chikaliuk",
            "email": "sharapov.k44@gmail.com",
            "homepage": "https://github.com/lirik44"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "phpunit/phpunit": "^7.1|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PHPUnitRetry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnitRetry\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2"
    }
}
